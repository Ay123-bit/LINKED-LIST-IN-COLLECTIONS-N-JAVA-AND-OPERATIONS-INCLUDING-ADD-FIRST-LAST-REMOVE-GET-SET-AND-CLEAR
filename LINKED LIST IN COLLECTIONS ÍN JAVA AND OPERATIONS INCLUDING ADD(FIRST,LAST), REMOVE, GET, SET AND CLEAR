//INPUT
import java.util.LinkedList;
class javaCollections{
    public static void main(String[] args){
        LinkedList<String> Name=new LinkedList<String>();
        Name.add("Ankit");
        Name.add("Ankush");
        System.out.println(Name);
        Name.addFirst("Suresh");
        System.out.println(Name);
        Name.addLast("Rakesh");
        System.out.println(Name);
        Name.add(1,"Rohit");
        System.out.println(Name);
        Name.remove(3);
        System.out.println(Name);
        Name.set(0,"Akhilesh");
        System.out.println(Name);
        System.out.println(Name.get(2));
        Name.clear();
        System.out.println(Name);
        for(String str:Name){
            System.out.println(str);
        }
    }
}
//LINKED LIST IN COLLECTIONS √çN JAVA AND OPERATIONS INCLUDING ADD(FIRST,LAST), REMOVE, GET, SET AND CLEAR
//OUPUT
[Ankit, Ankush]
[Suresh, Ankit, Ankush]
[Suresh, Ankit, Ankush, Rakesh]
[Suresh, Rohit, Ankit, Ankush, Rakesh]
[Suresh, Rohit, Ankit, Rakesh]
[Akhilesh, Rohit, Ankit, Rakesh]
Ankit
[]
